{
    "name": "katsana/katsana-sdk-laravel",
    "description": "KATSANA SDK for Laravel",
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "KATSANA Developer",
            "email": "developer@katsana.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Katsana\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Katsana\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1",
        "illuminate/support": "^5.8 || ^6.0",
        "katsana/katsana-sdk-php": "^1.2.4",
        "php-http/guzzle6-adapter": "^2.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3",
        "orchestra/testbench": "^3.8.5 || ^4.0.1"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        },
        "laravel": {
            "providers": [
                "Katsana\\ServiceProvider"
            ],
            "aliases": {
                "Katsana": "Katsana\\Katsana"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
