{
    "name": "katsana/katsana-sdk-laravel",
    "description": "KATSANA SDK for Laravel",
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "KATSANA Developer",
            "email": "developer@katsana.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Katsana\\": "src/"
        }
    },
    "require": {
        "php": ">=5.5",
        "illuminate/support": "^5.1",
        "katsana/katsana-sdk-php": "~0.2",
        "php-http/guzzle6-adapter": "^1.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "~1.11",
        "mockery/mockery": "^0.9.4",
        "phpunit/phpunit": "~5.7 || ~6.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev",
            "dev-develop": "0.2-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
