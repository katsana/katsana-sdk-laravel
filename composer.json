{
    "name": "katsana/katsana-sdk-laravel",
    "description": "KATSANA SDK for Laravel",
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "KATSANA Developer",
            "email": "developer@katsana.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Katsana\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Katsana\\TestCase\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1",
        "illuminate/support": "^5.8",
        "katsana/katsana-sdk-php": "^1.2",
        "php-http/guzzle6-adapter": "^1.1.1 || ^2.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^3.8"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        },
        "laravel": {
            "providers": [
                "Katsana\\ServiceProvider"
            ],
            "aliases": {
                "Katsana": "Katsana\\Katsana"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
